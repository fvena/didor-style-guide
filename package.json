  "exports": {
    "./eslint/*": "./eslint/*.js",
    "./prettier/*": "./prettier/index.js",
    "./stylelint/*": "./stylelint/index.js",
    "./typescript/*": "./typescript/tsconfig.base.json"
  },
  "main": "index.js",
  "files": [
    "eslint",
    "prettier",
    "stylelint",
    "typescript",
    "vitest"
