    "prettier": "^3.3.1",
    "semantic-release": "^24.0.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0 <9",
    "prettier": ">= 3.3.1 <4",
    "stylelint": "^16.6.1 <17",
    "typescript": ">= 5.4.5 <6"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "stylelint": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
