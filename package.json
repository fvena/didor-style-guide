  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@semantic-release/git": "^10.0.1",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.1",
    "semantic-release": "^24.0.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0 <9",
    "prettier": ">= 3.3.1 <4",
    "stylelint": "^16.6.1 <17",
    "typescript": ">= 5.4.5 <6"
