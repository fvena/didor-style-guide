    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-plugin-vue-pug": "^0.6.2",
    "prettier-plugin-packagejson": "^2.5.0",
    "stylelint-config-recess-order": "^5.0.1",
